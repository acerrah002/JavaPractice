public class Main {
    public static void main(String[] args) {
        Staircase("*",4);
    }
    public static void Staircase(String x, int y){
        for(int i=0;i<y;i++){
            for(int t=0;t<i;t++){
                System.out.print(x);
            }
            System.out.println(x);
        }
    }
}
